<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="IntHandler"
		class="nl.mindef.c2sc.nbs.olsr.pud.uplink.server.signals.impl.StopHandlerImpl"
		init-method="init" destroy-method="destroy">
		<property name="handlers">
			<set>
				<ref bean="DatabaseLoggerTimer"/>
				<ref bean="UplinkReceiver" />
				<ref bean="Distributor" />
				<ref bean="ExpiryNodesTimer"/>
			</set>
		</property>
		<property name="signal" value="INT" />
	</bean>

</beans>
