<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="PacketHandler"
		class="nl.mindef.c2sc.nbs.olsr.pud.uplink.server.handlers.impl.PacketHandlerImpl">
		<property name="clusterLeaderHandler" ref="ClusterLeaderHandler" />
		<property name="dataLock" ref="DaoDataLock" />
		<property name="faker" ref="Faker" />
		<property name="positionUpdateHandler" ref="PositionUpdateHandler" />
		<property name="relayServers" ref="RelayServers" />
		<property name="useFaker" value="${faker.use}" />
	</bean>

</beans>
