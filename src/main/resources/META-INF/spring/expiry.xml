<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="ExpiryNodes"
		class="nl.mindef.c2sc.nbs.olsr.pud.uplink.server.expiry.ExpireNodes"
		init-method="init" destroy-method="destroy">
		<property name="config" ref="RelayServerConfiguration" />
		<property name="interval" value="${node.expiry.interval}" />
		<property name="nodes" ref="Nodes" />
		<property name="positions" ref="Positions" />
		<property name="validityTimeMultiplier" value="${node.expiry.multiplier}" />
	</bean>

</beans>
