<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="ReportOnce"
		class="nl.mindef.c2sc.nbs.olsr.pud.uplink.server.reportonce.impl.ReportOnceImpl" />

	<bean id="ReportOnceTimer"
		class="nl.mindef.c2sc.nbs.olsr.pud.uplink.server.reportonce.ReportOnceTimer"
		init-method="init" destroy-method="uninit">
		<property name="intervalWireFormat" value="${report.once.interval.wireformat}" />
		<property name="intervalDuplicateNames" value="${report.once.interval.duplicate.names}" />
		<property name="reportOnce" ref="ReportOnce" />
	</bean>

</beans>
