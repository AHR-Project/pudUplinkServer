<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="UplinkReceiver"
		class="nl.mindef.c2sc.nbs.olsr.pud.uplink.server.uplink.UplinkReceiver"
		init-method="init" destroy-method="uninit">
		<property name="configuredRelayServers" value="${relaycluster.servers}" />
		<property name="databaseLogger" ref="DatabaseLogger" />
		<property name="databaseLoggerTimer" ref="DatabaseLoggerTimer" />
		<property name="distributor" ref="Distributor" />
		<property name="expireNodesTimer" ref="ExpiryNodesTimer" />
		<property name="packetHandler" ref="PacketHandler" />
		<property name="relayClusterSender" ref="RelayClusterSender" />
		<property name="relayServers" ref="RelayServers" />
		<property name="uplinkUdpPort" value="${uplink.listener.udp.port}" />
	</bean>

</beans>
