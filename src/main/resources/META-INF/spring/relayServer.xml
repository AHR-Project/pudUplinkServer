<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="RelayServer" class="nl.mindef.c2sc.nbs.olsr.pud.uplink.server.RelayServer"
		init-method="init" destroy-method="destroy">
		<property name="config" ref="RelayServerConfiguration" />
		<property name="nodes" ref="Nodes" />
		<property name="packetHandler" ref="PacketHandler" />
		<property name="positions" ref="Positions" />
		<property name="relayCluster" ref="RelayCluster" />
		<property name="relayServers" ref="RelayServers" />
		<property name="uplinkUdpPort" value="${uplink.listener.udp.port}" />
	</bean>

</beans>
