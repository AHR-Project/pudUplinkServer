<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<!-- Spring -->
	<bean
		class="org.springframework.beans.factory.annotation.RequiredAnnotationBeanPostProcessor" />

	<!-- Debug Beans -->
	<import resource="classpath:META-INF/spring/faker.xml" />

	<!-- Beans -->
	<import resource="classpath:META-INF/spring/jmx.xml" />

	<import resource="classpath:META-INF/spring/stopHandler.xml" />

	<import resource="classpath:META-INF/spring/hibernate.xml" />
	<import resource="classpath:META-INF/spring/txChecker.xml" />
	<import resource="classpath:META-INF/spring/senders.xml" />
	<import resource="classpath:META-INF/spring/nodes.xml" />
	<import resource="classpath:META-INF/spring/positionUpdateMsgs.xml" />
	<import resource="classpath:META-INF/spring/clusterLeaderMsgs.xml" />
	<import resource="classpath:META-INF/spring/relayServers.xml" />
	<import resource="classpath:META-INF/spring/expiry.xml" />
	<import resource="classpath:META-INF/spring/databaseLogger.xml" />
	<import resource="classpath:META-INF/spring/reportOnce.xml"/>

	<import resource="classpath:META-INF/spring/clusterLeaderHandler.xml" />
	<import resource="classpath:META-INF/spring/positionUpdateHandler.xml" />
	<import resource="classpath:META-INF/spring/packetHandler.xml" />
	<import resource="classpath:META-INF/spring/wireFormatChecker.xml"/>

	<import resource="classpath:META-INF/spring/distributor.xml" />

	<import resource="classpath:META-INF/spring/uplinkReceiver.xml" />

	<import resource="classpath:META-INF/spring/myIPAddresses.xml" />

</beans>
