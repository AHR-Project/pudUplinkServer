<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="RelayCluster"
		class="nl.mindef.c2sc.nbs.olsr.pud.uplink.server.relaycluster.impl.RelayClusterImpl"
		init-method="init" destroy-method="destroy">
		<property name="config" ref="RelayServerConfiguration" />
		<property name="configuredRelayServers" value="${relaycluster.servers}" />
		<property name="distributionDelay" value="${distribution.delay}" />
		<property name="nodes" ref="Nodes" />
		<property name="packetMaxSize" value="${distribution.packet.max.size}" />
		<property name="positions" ref="Positions" />
		<property name="relayServers" ref="RelayServers" />
	</bean>

</beans>
   